{
    "executionRoleArn": "arn:aws:iam::781000631387:role/ecsCodeDeployRole",
    "containerDefinitions": [
      {
        "portMappings": [
          {
            "hostPort": 5000,
            "protocol": "tcp",
            "containerPort": 5000
          }
        ],

        "image": "<IMAGE1_NAME>",
        "essential": true,
        "name": "container-localhost-app"
      }
    ],

    "requiresCompatibilities": [
      "FARGATE"
    ],
    "networkMode": "awsvpc",
    "cpu": "512",
    "memory": "1024",
    "family": "task-localhost-app"
  }